    <splash density="land-mdpi" src="drawable-land-mdpi/screen.png" />
    <splash density="land-xhdpi" src="drawable-land-xhdpi/screen.png" />
    <splash density="port-hdpi" src="drawable-port-hdpi/screen.png" />
